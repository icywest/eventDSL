Model:
    events+=Event
;

// Evento concreto a guardar en BD / JSON / calendar
Event:
    'add' 'event' name=STRING
    'for' requester_type=RequesterType
    ('requested_by' requester_unit=RequesterUnitOption)?
    'on' date=DATE
    'from' start=TIME
    'to' end=TIME
    'at' location=LocationOption
    ('description' description=STRING)?
;

// Tipos de solicitante (matching con rules_dsl)
RequesterType:
      'Academics'
    | 'Students'
;

// Locations (matching con rules_dsl)
LocationOption:
      'PellasRoom'
    | 'NewAuditorium'
    | 'PoolArea'
    | 'SB116'
    | 'LibraryRoom'
    | 'REC'
    | 'Other'
;

// Unidades / clubes (matching con rules_dsl)
RequesterUnitOption:
      'BusinessClub'
    | 'PsychologyClub'
    | 'PoliticsAndDebateClub'
    | 'SoftwareClub'
    | 'MathAndScienceClub'
    | 'MarketingClub'
    | 'GamingClub'
    | 'VerdeTeQuieroVerdeClub'
    | 'MusicClub'
    | 'CreativePadClub'
    | 'GymClub'
    | 'LArcheClub'
    | 'DanceHawksOrganization'
    | 'ShiningStarsOrganization'
    | 'AnythingIsPawsibleVolunteering'
    | 'TutoringProgram'
    | 'PilasPuestas'
    | 'AEISEC'
    | 'MissionTrip'
    | 'GuadalupeProject'
    | 'ConsecrationToJesusThruMary'
    | 'RosaMistica'
    | 'TheNewCovenant'
    | 'InciensoDeDios'
    | 'Angelus'
    | 'XthLegionOfChrist'
    | 'GuardiansOfTheSacredHeart'
    | 'FellowshipOfTheCross'
;

// Terminales
DATE: /\d{4}-\d{2}-\d{2}/;
TIME: /\d{2}:\d{2}/;
INT:  /\d+/;
STRING: /"[^"]*"/;
